sumUpTo(n::Int)::Int
    sum := 0
    for i from 1 to n do
        sum = sum + i
    end
    return sum
end

sumUpTo(n::Float)::Float
    sum := 0.0
    i := 1.0
    while i <= n do
        sum = sum + i
        i = i + 1.0
    end
    return sum
end

script miniTypes
    x := sumUpTo(100.0)
    y := sumUpTo(100)
    print(x)
    print(y)
end