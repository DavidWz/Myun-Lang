script testWhile
    print(1)
    while false do
        print(2)
    end
    print(3)
    // 1, 3

    x := 3
    y := 0
    while x > 0 do
        x = x - 1
        y = y + 1
    end
    print(y) // 3

    a := 10
    c := 0
    while a > 0 do
        b := a
        while b > 0 do
            c = c + 1
            b = b - 1
        end
        a = a-1
    end
    print(c) // 55

    t := 10
    while t > 0 do
        t = t - 1
        if t is 5 then
            break
        else
            print(t)
        end
    end
    // 9, 8, 7, 6
end