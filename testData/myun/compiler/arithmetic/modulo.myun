script testMod
    // ints
    print(22 mod 5)      // 2
    print(-491 mod 11)   // -7
    print(39 mod -4)     // 3
    print(-192 mod -100) // -92

    // floats
    print(2.3 mod 1.1)   // 0.1
    print(-1.1 mod 0.3)  // -0.2
    print(9.0e-2 mod 3.3e-3) // 9e-4
    print(-100.0e10 mod -50.0e7) // 0
end