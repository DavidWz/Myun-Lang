script testDiv
    // ints
    print(10 / 2)         // 5
    print(-20 / 4)        // -5
    print(1 / 2390242)    // 0
    print(100 / 32)       // 3
    print(9817272 / 128 / 18 / 32)  // 133

    // floats
    print(-3.0 / 1.5)         // -2.0
    print(932.0e-5 / 12.1e2)  // 7.70247933e-6
    print(0.0 / 43.0)         // 0.0
    print(1.0 / 2.2 / 0.03 / -4.0 / -1.01e4) // 3.750375038e-4
    print(3.0 / 0.0)          // +inf
end