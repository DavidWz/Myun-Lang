script testAdd
    // ints
    print(5 + 5)           // 10
    print(-3992 + 99)      // −3893
    print(389203 + -32472) // 356731
    print(1+2+3+4+5)       // 15

    // floats
    print(2.1 + 0.9)       // 3.0
    print(-31.2 + 98.0)    // 66.8
    print(0.001 + -302.4)  // −302.399
    print(2.3e-5 + 7.0E-6) // 0,00003 = 3.0e-5
    print(2.997e7 + 300.0e2) // 30000000.0 = 3.0e7
    print(1.0 + 3.0e1 + 2.0e2 + 1.0e-1 + 0.001) // 231.101
end